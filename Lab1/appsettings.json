{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=app.sqlite"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
